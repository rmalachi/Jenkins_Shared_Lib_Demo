@Library ('vtg_shared_lib') _

import tpl.python.runners.tplGenericPythonRunner

pipeline
{    
    agent any
    
    stages
    {
        stage('Template Test Run')
        {
            steps
            {
                script
                {
                    def runner = new tplGenericPythonRunner(this)
                    runner.run()
                }
            }
        }
    }
}
